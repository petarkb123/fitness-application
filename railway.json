{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "java -Dspring.profiles.active=prod -jar target/FitnessApplication-0.0.1-SNAPSHOT.jar",
    "healthcheckPath": "/actuator/health"
  },
  "environments": {
    "production": {
      "variables": {
        "SPRING_PROFILES_ACTIVE": "prod"
      }
    }
  }
}
